<object class = "XMLRPCServer" role = "XMLRPCServer">
 
  <!-- Should XML-RPC server listen on all interfaces?
       False (the default) means listen only on the interface given
       by socket.gethostname()-->
   
    <all_interfaces> 
        True
    </all_interfaces>
    <port>
        60000
    </port>

	  <apis>
	    <api>
	     <module>Native</module>
	     <recurse>True</recurse>
	   </api>
	  </apis>

    <object href="/singleton_objects/queue" role="queue"/>
    <object href="/singleton_objects/queue-model" role="queue_model"/>
</object>
